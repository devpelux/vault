    <Style x:Key="ComboBoxPlusDark" TargetType="{x:Type fc:ComboBoxPlus}">
        <Setter Property="Background" Value="#FF3F3F46"/>
        <Setter Property="BackgroundOnMouseOver" Value="#FF575759"/>
        <Setter Property="BackgroundOnMouseOverOnChecked" Value="#FF2D2D30"/>
        <Setter Property="BackgroundOnChecked" Value="#FF2D2D30"/>
        <Setter Property="BackgroundOnDisabled" Value="#FF3F3F46"/>
        <Setter Property="BorderBrush" Value="#FF575759"/>
        <Setter Property="BorderBrushOnMouseOver" Value="#FF575759"/>
        <Setter Property="BorderBrushOnMouseOverOnChecked" Value="#FF575759"/>
        <Setter Property="BorderBrushOnChecked" Value="#FF575759"/>
