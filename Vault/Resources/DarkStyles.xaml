        <Setter Property="BorderBrushOnMouseOver" Value="{DynamicResource WindowDivider}"/>
        <Setter Property="BorderBrushOnChecked" Value="#FF19478B"/>
        <Setter Property="BorderBrushOnMouseOverOnChecked" Value="#FF19478B"/>
