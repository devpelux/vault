        <Setter Property="ForegroundOnMouseOver" Value="WhiteSmoke"/>
        <Setter Property="ForegroundOnPressed" Value="WhiteSmoke"/>
        <Setter Property="ForegroundOnDisabled" Value="Gray"/>
    </Style>

    <Style x:Key="LogoutButtonDark" TargetType="{x:Type fc:ButtonPlus}" BasedOn="{StaticResource SettingsButtonDark}">
        <Setter Property="BackgroundOnPressed" Value="Red"/>
        <Setter Property="BorderBrushOnPressed" Value="Red"/>
    </Style>

    <Style x:Key="SwitcherDark" TargetType="{x:Type fc:Switcher}">
