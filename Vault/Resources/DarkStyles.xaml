    <Style x:Key="ComboBoxItemPlusDark" TargetType="{x:Type fc:ComboBoxItemPlus}">
        <Setter Property="Background" Value="#FF2D2D30"/>
        <Setter Property="BackgroundOnMouseOver" Value="#FF575759"/>
        <Setter Property="BackgroundOnSelected" Value="#FF0099FF"/>
        <Setter Property="BackgroundOnMouseOverOnSelected" Value="#FF0089E4"/>
