        <Setter Property="ForegroundOnMouseOver" Value="WhiteSmoke"/>
        <Setter Property="ForegroundOnSelected" Value="WhiteSmoke"/>
        <Setter Property="ForegroundOnMouseOverOnSelected" Value="WhiteSmoke"/>
        <Setter Property="ForegroundOnFocused" Value="WhiteSmoke"/>
        <Setter Property="ForegroundOnFocusedOnSelected" Value="WhiteSmoke"/>
        <Setter Property="ForegroundOnDisabled" Value="Gray"/>
