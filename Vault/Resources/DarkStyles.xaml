        <Setter Property="BorderBrush" Value="#FF434346"/>
        <Setter Property="BorderBrushOnSelected" Value="#FF0099FF"/>
        <Setter Property="BorderBrushOnDisabled" Value="#FF434346"/>
        <Setter Property="Foreground" Value="WhiteSmoke"/>
