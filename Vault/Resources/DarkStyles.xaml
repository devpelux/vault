    <Style x:Key="SettingsButtonDark" TargetType="{x:Type fc:ButtonPlus}">
        <Setter Property="Background" Value="#FF1C1C1C"/>
        <Setter Property="BackgroundOnMouseOver" Value="{DynamicResource WindowDivider}"/>
        <Setter Property="BackgroundOnPressed" Value="#FF2D2D30"/>
        <Setter Property="BackgroundOnDisabled" Value="#FF1A1A1A"/>
        <Setter Property="BorderBrush" Value="{DynamicResource WindowDivider}"/>
        <Setter Property="BorderBrushOnMouseOver" Value="{DynamicResource WindowDivider}"/>
        <Setter Property="BorderBrushOnPressed" Value="#FF2D2D30"/>
        <Setter Property="BorderBrushOnDisabled" Value="#FF1C1C1C"/>
